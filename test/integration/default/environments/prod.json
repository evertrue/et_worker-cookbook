{
  "name": "prod",
  "description": "The production environment",
  "cookbook_versions": {
    "et_api_haproxy": "= 2.4.3",
    "et_ops_haproxy": "= 2.2.4",
    "et_auth_api": "= 2.0.2",
    "et_base": "= 1.11.1",
    "et_contacts_api": "= 1.4.2",
    "et_contacts_search": "= 1.1.1",
    "et_ec2dnsserver": "= 1.2.0",
    "et_importer_api": "= 1.1.1",
    "et_lids_api": "= 1.0.4",
    "et_reds_api": "= 1.2.5",
    "et_reporting_api": "= 1.3.5",
    "et_scout_api": "= 1.0.3",
    "et_search_api": "= 1.4.1",
    "et_storm_nimbus": "= 1.0.4",
    "et_storm_supervisor": "= 1.1.0",
    "et_worker": "= 1.2.8",
    "et_zookeeper": "= 1.1.6",
    "java": ">= 1.16.2",
    "logserver": "= 1.5.0"
  },
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "default_attributes": {
  },
  "override_attributes": {
    "chef_env_long_name": "production",
    "probymonitor": {
      "task_id": "1f363070f6f30130c7e54040217846b4"
    },
    "ops_haproxy": {
      "acls": {
        "host_logs": {
          "type": "hdr_beg(host)",
          "match": "logs.evertrue.com"
        }
      },
      "frontends": {
        "main": {
          "port": "8080",
          "ports": ["8080"],
          "ssl": false
        },
        "main_ssl": {
          "port": "8443",
          "ports": ["8443"],
          "ssl": true
        }
      },
      "applications": {
        "logs": {
          "acls": [
            [
              "host_logs"
            ]
          ],
          "ssl_enabled": false,
          "ssl_required": false,
          "backend": "logs"
        }
      },
      "backends": {
        "logs": {
          "balance_algorithm": "roundrobin",
          "servers_recipe": "logserver",
          "port": "80"
        }
      }
    },
    "api_haproxy": {
      "acls": {
        "host_api": {
          "type": "hdr_beg(host)",
          "match": "api.evertrue.com"
        },
        "host_storm": {
          "type": "hdr_beg(host)",
          "match": "storm.evertrue.com"
        },
        "uri_search": {
          "type": "path_beg",
          "match": "/search"
        },
        "uri_contacts": {
          "type": "path_beg",
          "match": "/contacts"
        },
        "uri_reds": {
          "type": "path_beg",
          "match": "/reds"
        },
        "uri_auth": {
          "type": "path_beg",
          "match": "/auth"
        },
        "uri_reporting": {
          "type": "path_beg",
          "match": "/reporting"
        },
        "uri_lids": {
          "type": "path_beg",
          "match": "/lids"
        },
        "uri_importer": {
          "type": "path_beg",
          "match": "/importer"
        },
        "uri_scout": {
          "type": "path_beg",
          "match": "/scout"
        },
        "uri_10": {
          "type": "path_beg",
          "match": "/1.0"
        },
        "uri_01": {
          "type": "path_beg",
          "match": "/0.1"
        },
        "uri_ping": {
          "type": "path_beg",
          "match": "/ping"
        },
        "uri_docs": {
          "type": "path_beg",
          "match": "/docs"
        },
        "uri_tools": {
          "type": "path_beg",
          "match": "/tools"
        }
      },
      "frontends": {
        "main": {
          "port": "8080",
          "ports": ["8080"],
          "ssl": false
        },
        "main_ssl": {
          "port": "8443",
          "ports": ["8443"],
          "ssl": true
        }
      },
      "applications": {
        "searchapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_search"
            ]
          ],
          "endpoint": "searchapi.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "searchapi-prod"
        },
        "contactsapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_contacts"
            ]
          ],
          "endpoint": "contactsapi.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "contactsapi-prod"
        },
        "redsapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_reds"
            ]
          ],
          "endpoint": "redsapi.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "redsapi-prod"
        },
        "authapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_auth"
            ]
          ],
          "endpoint": "auth.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "authapi-prod"
        },
        "reportingapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_reporting"
            ]
          ],
          "endpoint": "reporting.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "reportingapi-prod"
        },
        "lidsapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_lids"
            ]
          ],
          "endpoint": "lids.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "lidsapi-prod"
        },
        "importerapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_importer"
            ]
          ],
          "endpoint": "importer.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "importerapi-prod"
        },
        "scoutapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_scout"
            ]
          ],
          "endpoint": "scout.evertrue.com",
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "scoutapi-prod"
        },
        "legacyapi-prod": {
          "acls": [
            [
              "host_api",
              "uri_10"
            ],
            [
              "host_api",
              "uri_01"
            ],
            [
              "host_api",
              "uri_ping"
            ],
            [
              "host_api",
              "uri_docs"
            ],
            [
              "host_api",
              "uri_tools"
            ]
          ],
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "legacyapi-prod"
        },
        "storm": {
          "acls": [
            [
              "host_storm"
            ]
          ],
          "access_control": true,
          "allowed": {
            "host_groups": [
              "set_global",
              "eips",
              "instance_ext_ips"
            ]
          },
          "ssl_enabled": true,
          "ssl_required": true,
          "backend": "storm_nimbus"
        }
      },
      "backends": {
        "searchapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/search/"
          },
          "port": "8080",
          "servers_recipe": "et_search_api"
        },
        "storm_nimbus": {
          "servers_recipe": "et_storm_nimbus",
          "port": "8080"
        },
        "contactsapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/contacts/"
          },
          "port": "8080",
          "servers_recipe": "et_contacts_api"
        },
        "redsapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/reds/",
            "always": true
          },
          "port": "8080",
          "servers_recipe": "et_reds_api"
        },
        "authapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/auth/",
            "always": true
          },
          "port": "8080",
          "servers_recipe": "et_auth_api"
        },
        "reportingapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/reporting/",
            "always": true
          },
          "port": "8080",
          "servers_recipe": "et_reporting_api"
        },
        "lidsapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/lids/",
            "always": true
          },
          "port": "8080",
          "servers_recipe": "et_lids_api"
        },
        "importerapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/importer/",
            "always": true
          },
          "port": "8080",
          "servers_recipe": "et_importer_api"
        },
        "scoutapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "method": "OPTIONS",
            "url": "/scout/",
            "always": true
          },
          "port": "8080",
          "servers_recipe": "et_scout_api"
        },
        "legacyapi-prod": {
          "balance_algorithm": "roundrobin",
          "check_req": {
            "always": true
          },
          "servers": [
            {
              "name": "prod-api-1",
              "fqdn": "10.0.113.215",
              "port": "8080"
            },
            {
              "name": "prod-api-2",
              "fqdn": "10.0.111.6",
              "port": "8080"
            }
          ]
        }
      }
    }
  }
}
